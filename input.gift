	~<p>\\(<br>\\forall y \\exists x \\colon \\lnot P(x,y)<br>\\)</p>
	~<p>\\(<br>\\exists x \\forall y\\colon P(x,y)<br>\\)</p>
	~<p>\\(<br> \\forall y \\exists x \\colon P(x,y)<br>\\)</p>
}


